#include <stdio.h>

/* EntrÃ©e : Tableau d'entiers et sa taille n
 * Sortie : Affichage du tableau    */

void print(int t[], int n) {
	int i ;

	for(i=0;i<n;i++) printf("%d",t[i]) ;
}

void appartient_grp_01(int x, int *T1, int s1, int n1) {
	int bol , i ;

	bol = 0 ;
	i = 0 ;
	while ((bol==0)&&(i<n1)) {
		if (x==*T1) bol = 1 ;
		i++ ;
		*T1++ ;
	}
	if ((bol==1)||(i<n)) printf("L'element %d appartient a l'ensemble",x) ;
	else printf("L'element %d n'appartient pas a l'ensemble",x) ;
}

/*Entree : 2 tableau avec le nombre d'element de chaque 
 *Sortie : savoir si l'un des deux est inclus dans l'autre */
 
void inclus_grp_01(int *T1, int *T2, int n1, int n2, int s1, int s2) {
	int compteur, i, j, tampon ;

	i = 0 ;
	compteur = 0 ;
	if (n1<n2) { 						/* On regarde quel est le tableau le plus petit */
		tampon = *T2 ; 					/* on sauvegarde l'adresse du premier element du plus grand tableau */
		while ((bol==1)&&(i<n1)) {
			*T2 = tampon ;				/* on reinitialise le pointeur au premier element */
			for(j=0;j<n2;j++) {
				if (*T1 == *T2) compteur++ ; 
				*T2++ ;
			}
			i++
			*T1++
		}
	else {
		tampon = *T1 ; 
		while ((bol==1)&&(i<n2)) {
			*T1 = tampon ;	
			for(j=0;j<n1;j++) {
				if (*T1 =! *T2) compteur++ ;
				*T1++ ;
			}
			i++
			*T2++
		}
	}
	if (compteur == n1) printf("Le tableau T1 est inclus dans le tableau T2");
	else if (compteur == n2) printf("Le tableau T2 est inclus dans le tableau T1");
	else printf("Aucun tableau n'est inclus dans l'autre");
}
